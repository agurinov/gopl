{
	"logger_j": {
		"format": "json"
	},
	"probes_j": {
		"check_interval": 3000000000
	},
	"graceful_j": {
		"shutdown_timeout": 2000000000
	},
	"grpc_j": {
		"port": 9090,
		"reflection_enabled": true
	},
	"debug_http_j": {
		"port": 8081
	},
	"map_scalar_j": {
		"foo": "bar",
		"lol": "kek"
	},
	"map_struct_j": {
		"foo": {
			"key": "key1"
		}
	},
	"array_scalar_j": [
		"foo",
		"bar"
	],
	"array_struct_j": [
		{
			"key": "key1",
			"value": "value1"
		},
		{
			"key": "key2",
			"value": "value2"
		}
	]
}
